{
  "name": "logrocket-nextjs-e2e-typesafe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "concurrently \"npm run generate:nexus\" \"npm run generate:genql\"",
    "generate:nexus": "nodemon --exec 'ts-node --transpile-only -P nexus.tsconfig.json pages/api/graphql' --ext 'ts' --watch '*/graphql/**/*.ts'",
    "generate:genql": "nodemon --exec 'genql --schema ./graphql/schema.graphql --output ./graphql/generated/genql'  --watch 'graphql/schema.graphql'"
  },
  "dependencies": {
    "@genql/runtime": "2.6.0",
    "@prisma/client": "2.30.3",
    "apollo-server-micro": "2.25.2",
    "firebase": "^9.6.7",
    "firebaseui": "^6.0.0",
    "graphql": "15.5.2",
    "graphql-scalars": "1.10.0",
    "next": "11.1.2",
    "nexus": "1.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.0.0"
  },
  "devDependencies": {
    "@genql/cli": "2.6.0",
    "@types/react": "17.0.20",
    "concurrently": "6.2.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "nodemon": "2.0.12",
    "prisma": "2.30.3",
    "ts-node": "10.2.1",
    "typescript": "4.4.2"
  }
}
